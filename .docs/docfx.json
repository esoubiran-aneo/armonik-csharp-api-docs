{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "ArmoniK.Core/**.csproj"
          ]
        }
      ],
      "dest": "api-armonik-core",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "ArmoniK.Extensions.Csharp/**.csproj"
          ]
        }
      ],
      "dest": "api-armonik-extensions-csharp",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "ArmoniK.Utils/**.csproj"
          ]
        }
      ],
      "dest": "api-armonik-utils",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
